{
  "name": "alicanto",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "check-types": "turbo run check-types",
    "clean": "find . -name \"node_modules\" -type d -prune -exec rm -rf {} +",
    "dev": "turbo run dev",
    "format": "pnpm biome format --write . ",
    "lint": "turbo run lint",
    "prepare": "husky",
    "test": "turbo run test --concurrency=2"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.6",
    "@types/node": "24.8.1",
    "husky": "9.1.7",
    "lint-staged": "16.2.4",
    "turbo": "2.5.8",
    "typescript": "5.9.3"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@10.18.3"
}
