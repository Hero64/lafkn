{
  "name": "@lafken/bucket",
  "version": "0.1.0",
  "private": false,
  "license": "MIT",
  "exports": {
    "./main": {
      "import": "./lib/main/index.js",
      "require": "./lib/main/index.js",
      "types": "./lib/main/index.d.ts"
    },
    "./service": {
      "import": "./lib/service/index.js",
      "require": "./lib/service/index.js",
      "types": "./lib/service/index.d.ts"
    },
    "./resolver": {
      "import": "./lib/resolver/index.js",
      "require": "./lib/resolver/index.js",
      "types": "./lib/resolver/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "main": [
        "./lib/main/index.d.ts"
      ],
      "service": [
        "./lib/service/index.d.ts"
      ],
      "resolver": [
        "./lib/resolver/index.d.ts"
      ]
    }
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "pnpm clean && tsc -p ./tsconfig.build.json",
    "clean": "rm -rf ./lib",
    "dev": "tsc -w",
    "prepublishOnly": "pnpm build",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.956.0",
    "@cdktf/provider-aws": "21.22.0",
    "@lafken/common": "workspace:*",
    "@lafken/resolver": "workspace:*",
    "aws-xray-sdk": "3.12.0",
    "cdktf": "0.21.0",
    "constructs": "10.4.4",
    "reflect-metadata": "0.2.2"
  },
  "devDependencies": {
    "@types/jest": "30.0.0",
    "jest": "30.2.0",
    "ts-jest": "29.4.6",
    "ts-node": "10.9.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
