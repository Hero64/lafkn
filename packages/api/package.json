{
  "name": "@lafken/api",
  "version": "0.6.2",
  "private": false,
  "description": "Build AWS REST APIs in the simplest possible way using TypeScript and API Gateway",
  "keywords": [
    "aws",
    "serverless",
    "api gateway",
    "lafken"
  ],
  "license": "MIT",
  "exports": {
    "./main": {
      "import": "./lib/main/index.js",
      "types": "./lib/main/index.d.ts",
      "require": "./lib/main/index.js"
    },
    "./resolver": {
      "import": "./lib/resolver/index.js",
      "types": "./lib/resolver/index.d.ts",
      "require": "./lib/resolver/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "main": [
        "./lib/main/index.d.ts"
      ],
      "resolver": [
        "./lib/resolver/index.d.ts"
      ]
    }
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "pnpm clean && tsc -p ./tsconfig.build.json",
    "clean": "rm -rf ./lib",
    "dev": "tsc -w",
    "prepublishOnly": "pnpm build",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@cdktf/provider-aws": "21.22.0",
    "@lafken/common": "workspace:*",
    "@lafken/resolver": "workspace:*",
    "cdktf": "0.21.0",
    "constructs": "10.4.4",
    "reflect-metadata": "0.2.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.159",
    "@types/jest": "30.0.0",
    "jest": "30.2.0",
    "ts-jest": "29.4.6",
    "ts-node": "10.9.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
